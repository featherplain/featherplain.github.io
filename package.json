  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "browser-sync": "^2.24.7",
    "node-sass": "^4.9.3",
    "postcss-cli": "^6.0.0",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6"
  }
}
