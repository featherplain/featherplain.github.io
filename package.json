{
  "name": "featherplain.github.io",
  "version": "0.0.1",
  "main": "index.html",
  "scripts": {
    "start": "node ./.bin/bs.js",
    "watch": "npm run pug & npm run node-sass",
    "pug": "pug ./src/pug/index.pug --out ./ -P -w",
    "node-sass": "node-sass ./src/scss/index.scss --output ./assets/css/ --output-style compressed -w -r; npx postcss ./assets/css/*.css --use autoprefixer -d ./assets/css/"
  },
  "repository": "git@github.com:featherplain/featherplain.github.io.git",
  "author": "featherplain <megumi@featherplain.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "browser-sync": "^2.24.7",
    "node-sass": "^4.9.3",
    "postcss-cli": "^6.0.0",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6"
  }
}
