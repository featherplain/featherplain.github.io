{
  "name": "featherplain.github.io",
  "version": "0.0.1",
  "main": "docs/index.html",
  "scripts": {
    "start": "node ./.bin/bs.js",
    "watch": "npm run pug & npm run node-sass",
    "pug": "pug ./src/pug/index.pug --out ./docs/ -P -w",
